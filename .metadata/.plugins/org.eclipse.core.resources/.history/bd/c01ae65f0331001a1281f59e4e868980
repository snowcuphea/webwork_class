<body>
<script type="text/javascript">

jumsu = 110;
if(jumsu<0 && jumsu >100){
	alert("잘못 입력");
	ch = '--'
}else if(jumsu>=80){
	ch = '수';
}else if(jumsu>=60 && jumsu<80){
	ch = '우';
}else if(jumsu>=40 && jumsu<60){
	ch = '미';
}else if(jumsu>=20 && jumsu<40){
	ch = '양';
}else{
	ch = '가';
}
switch(ch){

case '수' :
	document.write("<h1>환영합니다.(등급:"+ch+")</h1>");
	document.write("<img src = '../images/jang1.jpg'/>");
	break;
case '우' : 
	document.write("<h1>(등급:"+ch+")</h1>");
	document.write("<img src = '../images/jang1.jpg'/>");
	break;
case '미' : 
	document.write("<h1>(등급:"+ch+")</h1>");
	document.write("<img src = '../images/jang1.jpg'/>");
	break;
case '양' : 
	document.write("<h1>(등급:"+ch+")</h1>");
	document.write("<img src = '../images/jang1.jpg'/>");
	break;
case '가' : 
	document.write("<h1>(등급:"+ch+")</h1>");
	document.write("<img src = '../images/jang1.jpg'/>");
	break;
case '--' : 

	break;
	


}

</script>

</body>
</html>